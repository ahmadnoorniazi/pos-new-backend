{
	"name": "backend",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "node ./build/index.js",
		"build": "npm-run-all clean transpile",
		"server": "yarn run webpack && node ./app.bundle.js",
		"dev": "NODE_ENV=development npm-run-all build server",
		"prod": "NODE_ENV=production npm-run-all build server",
		"transpile": "babel ./src --out-dir build",
		"builds-ib": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
		"clean": "rimraf dist-server",
		"watch:dev": "nodemon",
		"webpack": "webpack"
	},
	"nodemonConfig": {
		"exec": "npm run dev",
		"watch": [
			"server/*",
			"public/*"
		],
		"ignore": [
			"**/__tests__/**",
			"*.test.js",
			"*.spec.js"
		]
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@babel/preset-env": "^7.8.7",
		"babel-preset-es2015": "^6.24.1",
		"babel-register": "^6.26.0",
		"nodemon": "^2.0.2",
		"rimraf": "^3.0.1",
		"webpack-cli": "^3.3.11"
	},
	"dependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/compat-data": "^7.14.7",
		"@babel/core": "^7.8.7",
		"@babel/plugin-proposal-object-rest-spread": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.8.3",
		"@babel/runtime": "^7.8.7",
		"babel-loader": "^8.1.0",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"cookie-parser": "^1.4.5",
		"core-js": "^3.6.4",
		"cors": "^2.8.5",
		"cron": "^1.8.2",
		"dateformat": "^3.0.3",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"find-process": "^1.4.3",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.15",
		"moment": "^2.24.0",
		"mongoose": "^5.8.11",
		"multer": "^1.4.2",
		"npm-run-all": "^4.1.5",
		"webpack": "^4.42.0"
	}
}